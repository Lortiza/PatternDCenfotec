@{
    ViewData["Title"] = "Temperature Conversions";
}

<h2>@ViewData["Title"]</h2>

<table>
    <thead>
        <tr>
            <th>Fahrenheit</th>
            <th>Celsius</th>
        </tr>
    </thead>
    <tbody>
        @{
            int start = (int)ViewData["Start"];
            int end = (int)ViewData["End"];
            int step = (int)ViewData["Step"];

            for (int fahrenheit = start; fahrenheit <= end; fahrenheit += step)
            {
                double celsius = (fahrenheit - 32) * 5.0 / 9.0;

                <tr>
                    <td>@fahrenheit</td>
                    <td>@celsius</td>
                </tr>
            }
        }
    </tbody>
</table>